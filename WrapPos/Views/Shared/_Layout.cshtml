<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ELaundry Solution</title>
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/css/datepicker3.css" rel="stylesheet">
    <link href="~/css/styles.css" rel="stylesheet">

    <link href="~/css/jquery-ui.css" rel="stylesheet" />


    <link href="~/css/jqbtk.css" rel="stylesheet" />
    <link href="~/css/sweet-alert.css" rel="stylesheet" />

    <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.5.0/css/bootstrap-datepicker.css" rel="stylesheet">
    <!--Custom Font-->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>

    <![endif]-->
    @RenderSection("head", false)
</head>
<body>


    <nav class="navbar navbar-custom navbar-fixed-top" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#sidebar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#"><span>ELaundry</span>Nepal</a>
                <ul class="nav navbar-top-links navbar-right">

                    <li class="dropdown">
                        <a class="dropdown-toggle count-info" data-toggle="dropdown" href="#">
                            <em class="fa fa-user-circle"></em>
                        </a>
                        <ul class="dropdown-menu dropdown-alerts">



                            <li class="divider"></li>
                            <li>
                                <a href="@Url.Action("Logout","Account")">
                                    <div>
                                        <em class="fa fa-user"></em> Logout

                                    </div>
                                </a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div><!-- /.container-fluid -->
    </nav>
    <div id="sidebar-collapse" class="col-sm-3 col-lg-2 sidebar">
        <div class="profile-sidebar">
            <div class="profile-userpic">
                <img src="http://placehold.it/50/30a5ff/fff" class="img-responsive" alt="">
            </div>
            <div class="profile-usertitle">
                @*<div class="profile-usertitle-name">@Session["fullname"].ToString()</div>*@


            </div>

            <div class="clear"></div>
        </div>
        <div class="divider"></div>
        <form role="search">
            <div class="form-group">
                @*<input type="text" class="form-control" readonly value="@Session["branchname"].ToString()">*@
            </div>
        </form>
        <ul class="nav menu">
            @if (User.IsInRole("Admin"))
            {
                <li id="home"><a href="@Url.Action("Dashboard", "Admin")"><em class="fa fa-dashboard">&nbsp;</em> Dashboard</a></li>
                <li id="category"><a href="@Url.Action("ManageCategory", "Category")"><em class="fa fa-history">&nbsp;</em> Category</a></li>
                <li id="garment"><a href="@Url.Action("ManageGarment", "Garment")"><em class="fa fa-user">&nbsp;</em> Garment</a></li>
                <li id="user"><a href="@Url.Action("ManageUser", "User")"><em class="fa fa-toggle-off">&nbsp;</em>User</a></li>
                <li><a href="@Url.Action("ChangePassword","Account")"><em class="fa fa-power-off">&nbsp;</em> Change Password</a></li>
                <li><a href="@Url.Action("SalesReport","Invoice")"><em class="fa fa-power-off">&nbsp;</em>User Sales Report</a></li>
                <li><a href="@Url.Action("ManageSetting","Setting")"><em class="fa fa-clone">&nbsp;</em> Settings</a></li>
                <li><a href="@Url.Action("MakeClosing","Closing")"><em class="fa fa-clone">&nbsp;</em>Make Closing</a></li>
            }
            @if (User.IsInRole("Collector"))
            {
                <li id="order"><a href="@Url.Action("ManageOrder", "Order")"><em class="fa fa-toggle-off">&nbsp;</em>Order</a></li>
                <li id="order"><a href="@Url.Action("OrderSearch", "OrderItem")"><em class="fa fa-toggle-off">&nbsp;</em>Search Order</a></li>
            }
            @if (User.IsInRole("Dhobi"))
            {
                <li><a href="@Url.Action("ViewDhobiOrder", "Dhobi")"><em class="fa fa-toggle-off">&nbsp;</em>Take Order from Customer</a></li>
                <li><a href="@Url.Action("#", "#")"><em class="fa fa-toggle-off">&nbsp;</em>View Customer Scheduled Data</a></li>
            }
            @if (User.IsInRole("Customer"))
            {
                <li><a href="@Url.Action("SchedulePickup", "Customer")"><em class="fa fa-toggle-off">&nbsp;</em>Schedule Pickup Now</a></li>
            }

            <li><a href="@Url.Action("Logout","Account")"><em class="fa fa-power-off">&nbsp;</em> Logout</a></li>

        </ul>
    </div><!--/.sidebar-->


    <div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">

        @RenderBody()
        <div class="row">
            <div class="col-sm-12">

                <p class="back-link">Copyright ELaundry <a href="#">@DateTime.Today.ToString("yyyy")</a></p>
            </div>
        </div><!--/.row-->

    </div>	<!--/.main-->


   
    <script src="~/js/jquery-3.4.1.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/jquery-ui-1.12.1.min.js"></script>
    <script src="~/js/jquery.validate.min.js"></script>
    <script src="~/js/jquery.validate.unobtrusive.min.js"></script>




    <script src="~/js/bootstrap-datepicker.js"></script>
    <script src="~/js/custom.js"></script>
    <script src="~/js/order.js"></script>

    <script src="~/js/jqbtk.js"></script>
    <script src="~/js/notify.min.js"></script>
    <script src="~/js/sweet-alert.js"></script>
    @RenderSection("scripts", required: false)

</body>
</html>
